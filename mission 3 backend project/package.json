  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "lint:check": "eslint . --ext .ts,.js",
    "list:fix": "eslint --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
